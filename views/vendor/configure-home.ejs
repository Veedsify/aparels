                  <form action="" class="col-sm-6 col-md-3">
                    <img src="/IMAGES/test.jpg" class="img-fluid img-thumbnail d-inline-block mb-2">
                    <h5 class="fw-bold">
                      Women Hero Banner
                    </h5>
                    <input type="file" class="form-control mb-2">
                    <input type="text" class="form-control mb-2" placeholder="Text One">
                    <input type="text" class="form-control mb-2" placeholder="Text Two">
                    <button type="submit" class="mb-2 btn btn-success w-100">Update</button>
                  </form>
                  <form action="" class="col-sm-6 col-md-3">
                    <img src="/IMAGES/test.jpg" class="img-fluid img-thumbnail d-inline-block mb-2">
                    <h5 class="fw-bold">
                      Trends Section Banner
                    </h5>
                    <input type="file" class="form-control mb-2">
                    <input type="text" class="form-control mb-2" placeholder="Text One">
                    <input type="text" class="form-control mb-2" placeholder="Text Two">
                    <input type="text" class="form-control mb-2" placeholder="Text Three">
                    <button type="submit" class="mb-2 btn btn-success w-100">Update</button>
                  </form>

                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-6"></div>
      </div>
      <!-- end page content -->
      <footer class="footer text-center text-sm-left">
        <div class="boxed-footer">
          &copy;
          <script>
            document.write(new Date().getFullYear());
          </script>
          <%- siteData.SITE_NAME %>
            <span class="text-muted d-none d-sm-inline-block float-right">Crafted with <i
                class="mdi mdi-heart text-danger"></i> by
              <a href="http://veedsify.com" target="_blank">Veedsify</a< /span>
        </div>
      </footer>
      <!--end footer-->
    </div>
    <!-- end page-wrapper -->

    <!-- jQuery  -->
    <script src="/ADMIN/js/jquery.min.js"></script>
    <script src="/ADMIN/js/bootstrap.bundle.min.js"></script>
    <script src="/ADMIN/js/metisMenu.min.js"></script>
