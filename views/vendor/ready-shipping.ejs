  Scrub caps">
  <meta name="author" content=Veedsify">
  <meta name="robots" content="index,follow">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta property="og:title" content="Medical Apparels">
  <meta property="og:description"
    content="Find the perfect medical apparel for your profession at Medical Apparels. We offer a wide range of comfortable and stylish medical scrubs, lab coats, shoes, and more. Browse our collection and discover the perfect fit for your needs. Shop now and experience the comfort and quality of our medical apparel.">
  <meta property="og:url" content="<%- siteLink %>">
  <meta property="og:image" content="<%- siteData.SITE_LOGO %>">
  <meta property="og:type" content="website">

  <!-- Facebook Meta Tags -->
  <meta property="og:title" content="Medical Apparels">
  <meta property="og:description"
    content="Find the perfect medical apparel for your profession at Medical Apparels. We offer a wide range of comfortable and stylish medical scrubs, lab coats, shoes, and more. Browse our collection and discover the perfect fit for your needs.">
  <meta property="og:url" content="<%- siteLink %>" <meta property="og:image" content="<%- siteData.SITE_LOGO %>">
  <meta property="og:type" content="website">

  <!-- Twitter Meta Tags -->
  <meta name="twitter:title" content="Medical Apparels">
  <meta name="twitter:description"
    content="Find the perfect medical apparel for your profession at Medical Apparels. We offer a wide range of comfortable and stylish medical scrubs, lab coats, shoes, and more. Browse our collection and discover the perfect fit for your needs.">
  <meta name="twitter:url" content="<%- siteLink %>">
  <meta name="twitter:image" content="<%- siteData.SITE_LOGO %>">
  <meta name="twitter:card" content="summary_large_image">

  <!-- App favicon -->
  <link rel="shortcut icon" href="<%- siteData.SITE_LOGO %>" />

  <!-- App css -->
  <link href="/ADMIN/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <link href="/ADMIN/css/icons.css" rel="stylesheet" type="text/css" />
  <link href="/ADMIN/css/metisMenu.min.css" rel="stylesheet" type="text/css" />
  <link href="/ADMIN/css/style.css" rel="stylesheet" type="text/css" />
</head>

<body>
  <!-- Top Bar Start -->
  <%- include('nav/top-bar') %>
    <!-- Top Bar End -->

    <div class="page-wrapper">
      <!-- Page Content-->
      <div class="page-content">
        <div class="container-fluid">
          <!-- Page-Title -->
          <div class="row">
            <div class="col-sm-12">
              <div class="page-title-box">
                <div class="float-right">
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                      <a href="javascript:void(0);">Medic Apparels</a>
                    </li>
                    <li class="breadcrumb-item">
                      <a href="javascript:void(0);">Orders</a>
                    </li>
                    <li class="breadcrumb-item active">Ready</li>
                  </ol>
                  <!--end breadcrumb-->
                </div>
                <!--end /div-->
                <h4 class="page-title">Ready For Shipping</h4>
              </div>
              <!--end page-title-box-->
            </div>
            <!--end col-->
          </div>
          <!--end row-->
          <div class="row">
            <% for(order of readyOrders) {%>
              <div class="col-lg-4">
                <div class="card">
                  <div class="card-body">
                    <!-- <h4 class="card-title mb-4">Pending Orders</h4> -->
                    <!-- Order Details -->
                    <div class="shadow shadow-sm p-3">
                      <!-- <img src="<%- order.ORDER_IMAGE %>" class="img-fluid d-block ob-fit-square" > -->
                      <div class="d-flex align-items-center justify-content-between">
                        <h4><%- order.ORDER_QUANTITY %> x <%- order.ORDER_NAME %></h4>
                        <div class="dropdown d-inline-block float-right">
                          <a class="nav-link dropdown-toggle mr-n2 mt-n2" id="drop3" data-toggle="dropdown" href="#"
                            role="button" aria-haspopup="false" aria-expanded="false">
                            <i class="fas fa-ellipsis-v text-muted"></i>
                          </a>
                          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="drop3">
                            <a class="dropdown-item shippedOrder" data-id="<%- order.ORDER_ID %>" href="">Mark As
                              Shipped</a>
                            <a class="dropdown-item cancelOrder" data-id="<%- order.ORDER_ID %>" href="">Cancel
                              Order</a>
                          </div>
                        </div>
                      </div>
                      <hr>
                      <div class="mt-3">
                        <p><strong> ID:</strong> <%- order.USER_ID %></p>
                        <p><strong>ORDER ID:</strong> <%- order.ORDER_ID %></p>
                        <p><strong>QUANTITY:</strong> <%- order.ORDER_QUANTITY %></p>
                        <p><strong>PRICE:</strong> â‚¦<%- Number(parseInt(order.FINAL_PRICE).toFixed(0)).toLocaleString()
                            %></p>
                        <p><strong>Description: </strong><%- order.ORDER_DESC %></p>
                        <p>Status: &nbsp; <strong class="btn btn-sm btn-soft-secondary text-capitalize"><%-
                              order.ORDER_STATUS %></strong></p>
                        <a href="/<%- user.ROLE %>/order/detail/<%- order.ORDER_ID %>" class="text-primary">More
                          details...</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <% } %>
          </div>
          <!-- end page title end breadcrumb -->
        </div>
        <!-- container -->
      </div>
      <!-- end page content -->
      <footer class="footer text-center text-sm-left">
        <div class="boxed-footer">
          &copy;
          <script>
            document.write(new Date().getFullYear());
          </script>
          Medic Apparels
          <span class="text-muted d-none d-sm-inline-block float-right">Crafted with <i
              class="mdi mdi-heart text-danger"></i> by
            <a href="http://veedsify.com" target="_blank">Veedsify</a< /span>
        </div>
      </footer>
      <!--end footer-->
    </div>
    <!-- end page-wrapper -->

    <!-- jQuery  -->
    <script src="/ADMIN/js/jquery.min.js"></script>
    <script src="/ADMIN/js/bootstrap.bundle.min.js"></script>
    <script src="/ADMIN/js/metisMenu.min.js"></script>
    <script src="/ADMIN/js/waves.min.js"></script>
    <script src="/ADMIN/js/jquery.slimscroll.min.js"></script>
    <script src="/CUSTOM/js/axios.min.js"></script>
    <script src="/CUSTOM/js/sweetalert.form.js"></script>
    <script src="/CUSTOM/js/sweetalert.min.js"></script>
    <script>const userRole = "<%- user.ROLE %>"</script>

    <!-- App js -->
    <script src="/ADMIN/js/app.js"></script>
    <script src="/CUSTOM/js/adminScript.js"></script>
</body>

</html>