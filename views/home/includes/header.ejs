<header>
  <div class="mobile-fix-option"></div>
  <div class="top-header">
    <div class="container">
      <div class="row">
        <div class="col-lg-6">
          <div class="header-contact">
            <ul>
              <li>Welcome to Our store <%- siteData.SITE_NAME %></li>
              <li>
                <i class="fa fa-phone" aria-hidden="true"></i>Call Us: <%- siteData.FOOTER_PHONE_CONTACT %>
              </li>
            </ul>
          </div>
        </div>
        <div class="col-lg-6 text-end">
          <ul class="header-dropdown">
            <li class="onhover-dropdown mobile-account">
              <% if (user) { %>
                <a href="/<%= user.ROLE %>"><i class="fa fa-user" aria-hidden="true"></i>
                  <%= user.NAME.split(" ")[0] %></a>
              <% } else { %>
                
               <a href=" /login"><i class="fa fa-user" aria-hidden="true"></i>Log in
                </a>
                <% } %>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <div class="main-menu">
          <div class="menu-left">
            <div class="brand-logo">
              <a href="/"><img loading="lazy" src="<%- siteData.SITE_LOGO %>" class="img-fluid blur-up  lazyload" alt=""
                  width="120" /></a>
            </div>
          </div>
          <div class="menu-right pull-right">
            <div>
              <nav id="main-nav">
                <div class="toggle-nav">
                  <i class="fa fa-bars sidebar-bar"></i>
                </div>
                <ul id="main-menu" class="sm pixelstrap sm-horizontal">
                  <li>
                    <div class="mobile-back text-end">
                      Back<i class="fa fa-angle-right ps-2" aria-hidden="true"></i>
                    </div>
                  </li>
                  <li><a href="/">Home</a></li>
                  <li>
                    <a href="/blog">Blog</a>
                  </li>
                  <li>
                    <a href="/shop">Shop <sup class="new-tag"><i class="fa fa-star"></i></sup></a>
                  </li>
                  <li>
                    <a href="/about">About</a>
                  </li>
                  <li>
                    <a href="#">More</a>
                    <ul>
                      <li><a href="/user">My Account</a></li>
                      <li><a href="/contact">Contact Us</a></li>
                    </ul>
                  </li>
                </ul>
              </nav>
            </div>
            <div>
              <div class="icon-nav">
                <ul>
                  <li class="onhover-div mobile-search">
                    <div>
                      <img loading="lazy" src="/assets/images/icon/search.png" onclick="openSearch()"
                        class="img-fluid blur-up  lazyload" alt="" />
                      <i class="ti-search" onclick="openSearch()"></i>
                    </div>
                    <div id="search-overlay" class="search-overlay">
                      <div>
                        <span class="closebtn" onclick="closeSearch()" title="Close Overlay">Ã—</span>
                        <div class="overlay-content">
                          <div class="container">
                            <div class="row">
                              <div class="col-xl-12">
                                <form>
                                  <div class="form-group">
                                    <input type="text" class="form-control" id="exampleInputPassword1"
                                      placeholder="Search a Product" />
                                  </div>
                                  <button type="submit" class="btn btn-primary">
                                    <i class="fa fa-search"></i>
                                  </button>
                                </form>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </li>
                  <li class="onhover-div mobile-cart">
                    <div>
                      <img loading="lazy" src="/assets/images/icon/cart.png" class="img-fluid blur-up  lazyload"
                        alt="" />
                      <i class="ti-shopping-cart"></i>
                    </div>
                    <span class="cart_qty_cls" id="cart-qtt"></span>
                    <ul class="show-div shopping-cart">
                      <span id="shopping-cart">


                      </span>

                      <li>
                        <div class="total">
                          <h5>subtotal : <span id="cart-total"></span></h5>
                        </div>
                      </li>
                      <li>
                        <div class="buttons">
                          <a href="/order/cart" class="view-cart">view cart</a>
                          <a href="/checkout" class="checkout">checkout</a>
                        </div>
                      </li>
                    </ul>
                  </li>
                  <li class="onhover-div mobile-setting">
                    <div><img loading="lazy" src="/IMAGES/list.svg" class="img-fluid blur-up  lazyloaded" width="30"> <i
                        class="ti-list"></i>
                    </div>
                    <div class="show-div setting">
                      <h6>More Pages</h6>
                      <ul>
                        <li><a href="/blog">Blog</a></li>
                        <li><a href="/shop">Shop</a></li>
                        <li><a href="/contact">Contact</a></li>
                        <li><a href="/about">About</a></li>
                      </ul>
                    </div>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>