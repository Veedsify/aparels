    <!-- Icons -->
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/font-awesome.css">

    <!--Slick slider css-->
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/slick.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/slick-theme.css">

    <!-- Animate icon -->
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/animate.css">

    <!-- Themify icon -->
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/themify-icons.css">

    <!-- Bootstrap css -->
    <link rel="stylesheet" type="text/css" href="/assets/css/vendors/bootstrap.css">

    <!-- Theme css -->
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">

    <%- include('../home/includes/head-extra') %>

</head>

<body class="theme-color-1" style="overflow: auto !important;">


    <!-- loader start -->
    <%- include('../home/includes/preloader') %>
        <!-- loader end -->

        <!-- header start -->
        <%- include('../home/includes/header') %>
            <!-- header end -->

            <div class="container">
                <div class="row my-5">
                    <div class="col-sm-12 col-md-6">
                        <div class="card border-0">
                            <div class="card-body">
                                <h3 class="card-title fw-bold mb-3">
                                    Account Details
                                </h3>
                                <form action="/user/account-info/update" id="accountDetails" class="row" method="post">
                                    <div class="form-group col-sm-12">
                                        <label class="d-block">
                                            Full Name:
                                            <input required type="text" value="<%- user.NAME  %>" class="form-control" id="name" name="name">
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="d-block">
                                            Email Address:
                                            <input required type="text" value="<%- user.EMAIL  %>" class="form-control"
                                                id="email" name="email">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label class="d-block">
                                            Country:
                                            <input required type="text" value="<%- user.COUNTRY  %>" class="form-control"
                                                id="country" name="country">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label class="d-block">
                                            State:
                                            <input required type="text" value="<%- user.STATE  %>" class="form-control"
                                                id="state" name="state">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label class="d-block">
                                            City:
                                            <input required type="text" value="<%- user.CITY  %>" class="form-control" id="city" name="city">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label class="d-block">
                                            Zip Code:
                                            <input required type="number" value="<%- user.ZIP_CODE  %>" class="form-control"
                                                id="zip" name="zip">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6 w-100">
                                        <label class="d-block">
                                            Street Address:
                                            <input required type="text" value="<%- user.ADDRESS  %>" class="form-control"
                                                id="street" name="street">
                                        </label>
                                    </div>
                                    <div class="form-group col-sm-6 w-100">
                                        <label class="d-block">
                                            Phone 1:
                                            <input required type="tel" class="form-control" value="<%- user.PHONE  %>"
                                                id="phone1" name="phone1">
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <button class="btn btn-sm btn-solid px-5 d-block">Save
                                            <i class="fa fa-save"></i>
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="card border-0">
                            <div class="card-body">
                                <h3 class="card-title fw-bold">
                                    Profile Image
                                </h3>

                                <form action="" class="d-flex flex-column justify-content-center align-items-center">
                                    <label class="d-block text-center">
                                        <img src="<%- user.PROFILE_IMAGE  %>" width="150" alt=""
                                            class="img-fluid img-thumbnail rounded-circle ob-fit-square d-block curp" />
                                        <small class="text-center">click to upload</small>
                                        <span class="fro-profile_main-pic-change">
                                            <input required type="file" name="profileImage" class="d-none" id="preview-file">
                                        </span>
                                    </label>
                                    <div class="form-group">
                                        <button class="btn btn-sm btn-solid">Upload</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

            <!-- footer start -->
            <%- include('../home/includes/footer') %>
                <!-- footer end -->


                <!-- latest jquery-->
                <script src="/assets/js/jquery-3.3.1.min.js"></script>

                <!-- menu js-->
                <script src="/assets/js/menu.js"></script>

                <!-- lazyload js-->
                <script src="/assets/js/lazysizes.min.js"></script>

                <!-- slick js-->
                <script src="/assets/js/slick.js"></script>

                <!-- Bootstrap js-->
                <script src="/assets/js/bootstrap.bundle.min.js"></script>

                <!-- Bootstrap Notification js-->
                <script src="/assets/js/bootstrap-notify.min.js"></script>

                <!-- Theme js-->
                <script src="/CUSTOM/js/axios.min.js"></script>
                <script src="/CUSTOM/js/sweetalert.form.js"></script>
                <script src="/CUSTOM/js/sweetalert.min.js"></script>

                <script src="/assets/js/script.js"></script>
                <script src="/CUSTOM/js/homeScript.js"></script>

                <script>
                    function openSearch() {
                        document.getElementById("search-overlay").style.display = "block";
                    }

                    function closeSearch() {
                        document.getElementById("search-overlay").style.display = "none";
                    }
                </script>

</body>

</html>